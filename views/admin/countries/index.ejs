<%# include ../helper/breadcrumb.ejs %>

<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <div class="box-header with-border">
                    <h3 class="box-title"> </h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">

                    <form id='searchData' name='searchData' enctype="multipart/form-data" method='post'>
                        <table class="table table-bordered">
                            <tr>
                                <th style="width: 10px">#Id</th>
                                <th style="width: 80px">Country</th>
                                <th>Alias</th>
                                <th>Code</th>
                                <th>Display Order</th>
                                <th>Active</th>
                                <th>Currency</th>
                                <th style="width: 100px">Actions</th>
                            </tr>
  

                            <% results.forEach(function(rows){ %>
                            <tr>
                                <td><%= rows.id %></td>
                                <td><%= rows.country_name %></td>
                                <td><%= rows.country_alias %></td>
                                <td><%= rows.country_code %></td>
                                <td><%= rows.display_order %></td>
                                <td>
                                    <% if(rows.is_active==1){ %>
                                    <span class="label label-success">Active</span>                               
                                    <% }else{ %>
                                    <span class="label label-danger">Inactive</span>
                                    <% } %>

                                </td>
								<td><%= rows.currency_id %></td>
                                <td>

                                    <a class="btn btn-success btn-xs" data-skin="skin-green" href="#" title="Edit" >
                                        <i class="fa fa-edit"></i></a>

                                    <a class="btn btn-success btn-xs blue" data-skin="skin-red" href="#" title="Reset Password"  ><i class="fa fa-edit"></i></a>

                                    <a class="btn btn-danger btn-xs" data-skin="skin-red" href="#" ><i class="fa <%= (rows.is_active==1)? 'fa-lock':'fa-unlock' %>"></i></a>
                                </td>

                            </tr>
                            <% }); %>


                        </table>
                        <%# include('../element/helper.ejs', {type:'empty_table',data:data}); %>
                    </form>
                </div>
                <!-- /.box-body -->
                <div class="box-footer clearfix">

                    <%# include ../helper/page2.ejs %>




                </div>
            </div>

        </div>

        <!-- /.col -->
    </div>
</section>